<div class="mt-20 mx-auto w-1/2 flex flex-col justify-center items-center bg-orange-400">
    <h1 class="text-4xl">HIGHSCORES</h1>
    <ul class="bg-yellow-200 w-1/2 mb-10">
        <% data.forEach(data => { %>
            <li class="flex justify-between"> 
                <p class="text-2xl"><%= data.name %></p>
                <p class="text-2xl font-semibold"><%= data.time %>s</p>
            </li>
        <% }); %> 
    </ul>
</div>